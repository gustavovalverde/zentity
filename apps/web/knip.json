{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    ".synpress-wallet-setup-dist/**",
    "src/components/ui/**",
    "src/components/liveness/**",
    "src/lib/liveness/**",
    "src/lib/zk/noir-worker-manager.ts",
    "scripts/**",
    "public/coi-serviceworker.js",
    "src/test/**",
    "e2e/**",
    "vitest.unit.config.mts"
  ],
  "ignoreDependencies": [
    "@vladmandic/human-models",
    "tfhe",
    "pino-pretty",
    "lint-staged",
    "@synthetixio/synpress-cache",
    "fs-extra",
    "@types/fs-extra"
  ],
  "ignoreBinaries": ["nargo"],
  "entry": [
    "src/app/**/page.{ts,tsx}",
    "src/app/**/layout.{ts,tsx}",
    "src/app/**/route.{ts,tsx}",
    "src/**/*.test.{ts,tsx}",
    "src/**/*.integration.test.{ts,tsx}"
  ],
  "ignoreExportsUsedInFile": true
}
