{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "24.x",
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "mkdir -p dist/plugins/two-factor && chmod -R 755 dist 2>/dev/null || true",
    "setup:coep-assets": "pnpm exec tsx scripts/setup-coep-assets.ts",
    "dev": "pnpm run setup:coep-assets && node --import tsx server.mjs",
    "dev:demo": "BETTER_AUTH_URL=http://host.docker.internal:3000 pnpm run dev",
    "build": "pnpm run setup:coep-assets && next build",
    "start": "node --import tsx server.mjs",
    "clean": "rm -rf .next test-results playwright-report coverage",
    "lint": "biome check . --write",
    "lint:check": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "lint:format": "biome format . --write",
    "lint:md": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\" --fix",
    "lint:md:check": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "check-all": "pnpm run type-check && pnpm run lint:check && pnpm run lint:md:check && pnpm run build && pnpm run circuits:check-versions",
    "fix-all": "pnpm run type-check && pnpm run lint && pnpm run lint:md",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "format": "biome format --write .",
    "test": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run",
    "test:watch": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs",
    "test:ui": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run --coverage",
    "test:e2e:setup": "pnpm exec tsx e2e/automation/build-synpress-cache.ts",
    "test:e2e": "pnpm run test:e2e:setup && pnpm exec playwright test",
    "test:e2e:ui": "pnpm run test:e2e:setup && pnpm exec playwright test --ui",
    "test:e2e:oidc": "E2E_OIDC_ONLY=true pnpm exec playwright test e2e/oidc",
    "test:e2e:oidc:ui": "E2E_OIDC_ONLY=true pnpm exec playwright test e2e/oidc --ui",
    "test:e2e:oidc:external": "E2E_OIDC_ONLY=true E2E_EXTERNAL_WEB_SERVER=true pnpm exec playwright test e2e/oidc",
    "db:push": "drizzle-kit push",
    "db:push:dev": "TURSO_DATABASE_URL=file:./.data/dev.db drizzle-kit push",
    "circuits:compile": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo compile && mkdir -p artifacts && cp -f target/*.json artifacts/); done",
    "circuits:test": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo test); done",
    "circuits:profile": "pnpm exec tsx scripts/noir-profile.ts",
    "circuits:profile:opcodes": "pnpm exec tsx scripts/noir-profile.ts opcodes",
    "circuits:profile:gates": "pnpm exec tsx scripts/noir-profile.ts gates",
    "circuits:profile:execution": "pnpm exec tsx scripts/noir-profile.ts execution-opcodes",
    "circuits:check-versions": "pnpm exec tsx scripts/check-noir-versions.ts",
    "prepare": "husky"
  },
