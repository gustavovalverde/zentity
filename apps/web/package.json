  "engines": {
    "node": "24.x",
    "bun": "1.3.x"
  },
  "scripts": {
    "preinstall": "mkdir -p dist/plugins/two-factor && chmod -R 755 dist 2>/dev/null || true",
    "setup:coep-assets": "bun scripts/setup-coep-assets.ts",
    "dev": "bun run setup:coep-assets && bun ./node_modules/next/dist/bin/next dev",
    "build": "bun run setup:coep-assets && bun ./node_modules/next/dist/bin/next build",
    "start": "bun ./node_modules/next/dist/bin/next start",
    "clean": "rm -rf .next test-results playwright-report coverage",
    "lint": "biome check . --write",
    "lint:check": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "lint:format": "biome format . --write",
    "lint:md": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\" --fix",
    "lint:md:check": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "check-all": "bun run type-check && bun run lint:check && bun run lint:md:check && bun run build && bun run circuits:check-versions",
    "fix-all": "bun run type-check && bun run lint && bun run lint:md",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "format": "biome format --write .",
    "test": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run",
    "test:watch": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs",
    "test:ui": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs run --coverage",
    "test:e2e:setup": "bun e2e/automation/build-synpress-cache.ts",
    "test:e2e": "bun run test:e2e:setup && bunx playwright test",
    "test:e2e:ui": "bun run test:e2e:setup && bunx playwright test --ui",
    "db:push": "drizzle-kit push",
    "db:push:dev": "TURSO_DATABASE_URL=file:./.data/dev.db drizzle-kit push",
    "circuits:compile": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo compile && mkdir -p artifacts && cp -f target/*.json artifacts/); done",
    "circuits:test": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo test); done",
    "circuits:profile": "bun scripts/noir-profile.ts",
    "circuits:profile:opcodes": "bun scripts/noir-profile.ts opcodes",
    "circuits:profile:gates": "bun scripts/noir-profile.ts gates",
    "circuits:profile:execution": "bun scripts/noir-profile.ts execution-opcodes",
    "circuits:check-versions": "bun scripts/check-noir-versions.ts",
    "prepare": "husky"
  },
