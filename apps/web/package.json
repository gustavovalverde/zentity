{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "24.x",
    "bun": "1.3.x"
  },
  "scripts": {
    "dev": "bun ./node_modules/next/dist/bin/next dev",
    "build": "bun ./node_modules/next/dist/bin/next build",
    "start": "bun ./node_modules/next/dist/bin/next start",
    "clean": "rm -rf .next test-results playwright-report coverage",
    "lint": "biome check . --write",
    "lint:check": "biome check .",
    "lint:fix": "biome check . --write --unsafe",
    "lint:format": "biome format . --write",
    "lint:md": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\" --fix",
    "lint:md:check": "markdownlint \"**/*.md\" --ignore \"node_modules/**/*.md\"",
    "type-check": "tsc --noEmit",
    "check-all": "bun run type-check && bun run lint:check && bun run lint:md:check",
    "fix-all": "bun run type-check && bun run lint && bun run lint:md",
    "format": "biome format --write .",
    "test": "bun ./node_modules/vitest/vitest.mjs",
    "test:ui": "bun ./node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "bun ./node_modules/vitest/vitest.mjs run --coverage",
    "test:e2e": "bunx playwright test",
    "test:e2e:ui": "bunx playwright test --ui",
    "circuits:compile": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo compile && mkdir -p artifacts && cp -f target/*.json artifacts/); done",
    "circuits:test": "cd noir-circuits && for d in */; do (cd \"$d\" && nargo test); done"
  },
  "dependencies": {
    "@aztec/bb.js": "^2.1.9",
    "@noir-lang/noir_js": "1.0.0-beta.17",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-form": "^1.27.4",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@trpc/client": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "@vladmandic/human": "^3.3.6",
    "@vladmandic/human-models": "^3.0.4",
    "better-auth": "^1.4.7",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.11.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "bun-types": "^1.3.5",
    "markdownlint-cli": "^0.47.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.2",
    "vitest": "^4.0.16"
  }
}
