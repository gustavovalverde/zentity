+import type {
+    TwoFactorMethod,
+    TwoFactorOptions
+} from "../types/two-factor-options"
+
+export type NormalizedTwoFactorOptions = {
+    methods?: TwoFactorMethod[]
+    allowPasswordless: boolean
+}
+
+export function normalizeTwoFactorOptions(
+    twoFactor?: TwoFactorOptions
+): NormalizedTwoFactorOptions {
+    if (!twoFactor) {
+        return { methods: undefined, allowPasswordless: false }
+    }
+
+    if (Array.isArray(twoFactor)) {
+        return { methods: twoFactor, allowPasswordless: false }
+    }
+
+    return {
+        methods: twoFactor.methods,
+        allowPasswordless: twoFactor.allowPasswordless ?? false
+    }
+}
